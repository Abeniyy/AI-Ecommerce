  "keywords": [
    "ecommerce",
    "ai",
    "react",
    "node",
    "python"
  ],
  "author": "Abenezer Tefera <abenibah@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "husky": "^9.0.0"
  }
}
